{
    "name": "local-concierge",
    "version": "1.0.0",
    "description": "Finding best solution is throw a question",
    "main": "www.js",
    "scripts": {
        "start": "run-p sub:webpack:dev:*",
        "build": "run-p sub:webpack:prod:*",
        "lint": "run-s sub:lint:*",
        "fix": "run-s sub:fix:*",
        "sub:lint:eslint": "eslint .",
        "sub:lint:style": "stylelint .",
        "sub:lint:prettier": "prettier -c .",
        "sub:fix:eslint": "npm run sub:lint:eslint --fix",
        "sub:fix:style": "npm run sub:lint:style --fix",
        "sub:fix:prettier": "npm run sub:lint:prettier --write",
        "sub:webpack:dev:client": "webpack --config ./webpack/dev/config.client.js",
        "sub:webpack:dev:server": "webpack --watch --config ./webpack/dev/config.server.js",
        "sub:webpack:prod:client": "webpack --config ./webpack/prod/config.client.js",
        "sub:webpack:prod:server": "webpack --config ./webpack/prod/config.server.js"
    },
    "keywords": [],
    "author": "anyone who wanna join this app",
    "license": "UNLICENSED",
    "dependencies": {
        "cookie-parser": "~1.4.6",
        "cors": "^2.8.5",
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "express-ejs-layouts": "^2.5.1",
        "helmet": "^5.1.1",
        "http-errors": "~2.0.0",
        "morgan": "~1.10.0",
        "mysql2": "^2.3.3",
        "sequelize": "^6.21.4"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.1",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "mini-css-extract-plugin": "^2.6.1",
        "nodemon-webpack-plugin": "^4.8.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "sass": "^1.54.4",
        "sass-loader": "^13.0.2",
        "stylelint": "^14.10.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recess-order": "^3.0.0",
        "stylelint-config-standard": "^27.0.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "stylelint-scss": "^4.3.0",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-middleware": "^5.3.3",
        "webpack-hot-middleware": "^2.25.2",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    }
}
